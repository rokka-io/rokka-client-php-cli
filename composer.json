{
  "name": "rokka/client-cli",
  "description": "CLI to use the Rokka service, using PHP",
  "keywords": [
    "rokka",
    "rokka.io"
  ],
  "homepage": "https://github.com/rokka-io/rokka-client-php-cli",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Emanuele Panzeri",
      "email": "emanuele.panzeri@liip.ch"
    }
  ],
  "require": {
    "php": "^5.6.0 || ^7.0.0",
    "rokka/client": "^1.0.0",
    "symfony/console" : "^2.7.0 || ^3.0.0 || ^4.0.0",
    "symfony/finder" : "^2.7.0 || ^3.0.0 || ^4.0.0",
    "symfony/config": "^2.7.0 || ^3.0.0 || ^4.0.0",
    "symfony/dependency-injection": "^2.7.0 || ^3.0.0 || ^4.0.0",
    "symfony/yaml": "^2.7.0 || ^3.0.0 || ^4.0.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.1"
  },
  "conflict": {
    "rokka/client-bundle": "<=1.0.1"
  },
  "autoload": {
    "psr-4": {
      "RokkaCli\\": "src/"
    }
  }
}
